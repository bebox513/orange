var button = "<%= @button %>";
if ("following" == button) {
  $('#followChange').html("<%= escape_javascript(render 'users/following') %>");
}else if("unfollow" == button) {
  $('#followChange').html("<%= escape_javascript(render 'users/unfollow') %>");
}else if ("followings" == button) {
  $('#mypageChange').html("<%= escape_javascript(render 'users/followings') %>");
}else if("followers" == button){
  $('#mypageChange').html("<%= escape_javascript(render 'users/followers') %>");
}else if("posts" == button){
  $('#mypageChange').html("<%= escape_javascript(render 'posts/posts') %>");
}
